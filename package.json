{
  "name": "webgazer",
  "version": "3.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/brownhci/WebGazer.git"
  },
  "license": "GPL-3.0-or-later",
  "module": "src/index.mjs",
  "type": "module",
  "scripts": {
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "dev": "webpack --progress --config webpack.config.js --mode development --watch",
