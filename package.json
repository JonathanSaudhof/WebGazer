  },
  "devDependencies": {
    "typescript": "^5",
    "@types/node": "^20",
    "browser-sync": "^3.0.2",
    "filemanager-webpack-plugin": "^8.0.0",
    "jsdoc": "^4.0.2",
    "rimraf": "2.6.3",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "yarnhook": "^0.4.3"
  }
}
